<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>@ViewData["Title"]</title>
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.1/css/all.css">
  <link rel="stylesheet" href="https://unpkg.com/bulmaswatch@0.7.2/slate/bulmaswatch.min.css">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma-calendar@6.0.7/dist/css/bulma-calendar.min.css">
  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true">
</head>
<body>

<div class="container is-fluid">

  <nav class="navbar" role="navigation" aria-label="main navigation">
    <div id="navbarBasicExample" class="navbar-menu">
      <div class="navbar-start">
        <a class="navbar-item" asp-controller="Home" asp-action="Index">
          Home
        </a>
        <a class="navbar-item" asp-controller="Enquiry" asp-action="Index">
          Enquiry
        </a>
        <a class="navbar-item" asp-controller="User" asp-action="Index">
          User
        </a>

      </div>

    </div>

  </nav>


</div>

<div class="container is-fluid">
  @RenderBody()
</div>

<script src="https://cdn.jsdelivr.net/npm/bulma-calendar@6.0.7/dist/js/bulma-calendar.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>

</body>
</html>